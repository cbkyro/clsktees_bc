{"components/home/carousel":"<section\n  class=\"\n    carousel\n    {{#if carousel.slides.length '===' 1}}\n      single-slide\n    {{/if}}\n    {{theme_settings.home-carousel-aspect-ratio}}\n  \"\n  data-swap-frequency=\"{{carousel.swap_frequency}}\"\n>\n  {{#each carousel.slides}}\n    <figure class=\"carousel-item {{#if @first}}active{{/if}}\">\n      {{#unless button_text}}\n        {{#if url}}<a href=\"{{url}}\">{{/if}}\n      {{/unless}}\n      <div class=\"carousel-item-image\" style=\"background-image: url('{{image}}');\">\n        <img class=\"home-carousel-image\" data-lazy=\"{{image}}\" alt=\"{{{alt_text}}}\" />\n      </div>\n      {{#unless button_text}}\n        {{#if url}}</a>{{/if}}\n      {{/unless}}\n      <figcaption\n        class=\"\n          carousel-item-info\n          text-position-{{../theme_settings.home_carousel_text_position}}\n        \"\n      >\n        {{#unless button_text}}\n          {{#if url}}\n            <a href=\"{{url}}\" class=\"carousel-item-link\"></a>\n          {{/if}}\n        {{/unless}}\n        {{#if heading}}\n          <h2\n            class=\"\n              carousel-item-title\n              caption-content\n              {{#if ../../theme_settings.home-carousel-show-slide-number}}\n                show-slide-number\n              {{/if}}\n            \"\n          >\n            {{{heading}}}\n          </h2>\n        {{/if}}\n        {{#if text}}\n          <p class=\"carousel-item-caption caption-content\">\n            {{{text}}}\n          </p>\n        {{/if}}\n        {{#if button_text}}\n          <a\n            class=\"button button-primary caption-content\"\n            href=\"{{url}}\"\n          >\n            {{{button_text}}}\n          </a>\n        {{/if}}\n        <span class=\"carousel-navigation-item previous\">\n          {{> svg/arrow-left}}\n        </span>\n        <span class=\"carousel-navigation-item next\">\n          {{> svg/arrow-right}}\n        </span>\n      </figcaption>\n    </figure>\n  {{/each}}\n</section>\n","svg/arrow-left":"<svg width=\"13\" height=\"22\" viewBox=\"0 0 13 22\" xmlns=\"http://www.w3.org/2000/svg\"><title>Arrow left</title><path d=\"M10.014 21.064L.938 12.05c-.584-.58-.584-1.52 0-2.1L10.014.937C10.3.653 10.68.5 11.074.5c.397 0 .777.153 1.063.436.583.58.583 1.52 0 2.1l-7.733 7.608-.362.356.362.356 7.734 7.61c.582.58.582 1.518-.002 2.098-.585.58-1.536.58-2.122 0z\" /></svg>\n","svg/arrow-right":"<svg width=\"13\" height=\"22\" viewBox=\"0 0 13 22\" xmlns=\"http://www.w3.org/2000/svg\"><title>Arrow right</title><path d=\"M12.136 9.95c.585.58.585 1.52 0 2.1L3.06 21.063c-.284.283-.665.436-1.06.436-.397 0-.777-.153-1.062-.436-.584-.58-.584-1.52 0-2.1l7.733-7.608.363-.356-.362-.356-7.733-7.61c-.583-.58-.583-1.518 0-2.098.586-.58 1.538-.58 2.124 0l9.076 9.015z\" /></svg>\n"}