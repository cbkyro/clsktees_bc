{"components/header/account-button":"{{#if customer}}\n  <div class=\"dropdown dropdown-account\" data-dropdown>\n    <a class=\"top-bar-button dropdown-toggle\" href=\"#\" data-dropdown-toggle>\n      <svg class=\"icon icon-account\"><use xlink:href=\"#icon-account\" /></svg>\n    </a>\n    <div class=\"dropdown-panel dropdown-panel-tooltip\" data-dropdown-panel>\n      <a class=\"account-link\" href=\"{{urls.account.index}}\">account</a>\n      <a class=\"account-link\" href=\"{{urls.auth.logout}}\">logout</a>\n    </div>\n  </div>\n{{else}}\n  <a class=\"top-bar-button\" href=\"{{urls.auth.login}}\">\n    <svg class=\"icon icon-account\"><use xlink:href=\"#icon-account\" /></svg>\n  </a>\n{{/if}}\n"}