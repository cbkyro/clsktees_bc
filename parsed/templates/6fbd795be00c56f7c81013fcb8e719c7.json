{"components/faceted-search/facets/hierarchy":"<div\n  class=\"facet-filter-wrapper\n  {{#unless start_collapsed}}is-open{{/unless}}\"\n  data-facet-filter-wrapper\n>\n  <ul class=\"facet-list-items facet-default\" data-count=\"{{items_count}}\">\n    {{#each items}}\n      <li class=\"facet-item\" data-facet-item>\n        <a\n          class=\"facet-item-link\"\n          href=\"{{url}}\"\n          rel=\"nofollow\"\n          data-faceted-search-facet\n        >\n          {{title}}\n          {{#if ../show_product_counts}}\n            <span class=\"facet-item-count\" data-facet-item-count>\n              ({{count}})\n            </span>\n          {{/if}}\n        </a>\n      </li>\n    {{/each}}\n  </ul>\n\n  {{#if show_more_toggle}}\n    <a\n      href=\"#\"\n      class=\"facet-toggle-more\"\n      data-facet-more=\"{{#replace ' ' facet}}{{else}}{{facet}}{{/replace}}\"\n    >\n      <span class=\"facet-item toggle-link-text-off is-open\">\n        {{lang 'faceted_search.show_more'}}\n        <svg class=\"icon icon-arrow-down\">\n          <use xlink:href=\"#icon-arrow-down\" />\n        </svg>\n      </span>\n      <span class=\"facet-item toggle-link-text-on\">\n        {{lang 'faceted_search.show_less'}}\n        <svg class=\"icon icon-arrow-up\">\n          <use xlink:href=\"#icon-arrow-up\" />\n        </svg>\n      </span>\n    </a>\n  {{/if}}\n"}