{"components/faceted-search/facets/rating":"<ul\n  class=\"\n    facet-list-items\n    {{#if selected}}\n      is-open\n    {{/if}}\n    {{#unless start_collapsed}}\n      is-open\n    {{/unless}}\n  \"\n  data-facet-filter-wrapper\n>\n  <li class=\"product-item-rating\" data-facet-item>\n    {{#for 1 4 items}}\n\n      {{#each this}}\n        {{#if param_value '==' ../$index}}\n          <a\n            class=\"\n              product-item-rating-facet\n              {{#if ../selected '==' true}}\n                full\n              {{/if}}\n            \"\n            href=\"{{url}}\"\n            rel=\"nofollow\"\n            data-faceted-search-facet\n            data-rating=\"{{lang 'faceted_search.stars' num=param_value}}\"\n          >\n        {{/if}}\n      {{/each}}\n\n      <svg class=\"icon icon-star \">\n        <use xlink:href=\"#icon-star\" />\n      </svg>\n\n      {{#each this}}\n        {{#if param_value '==' ../$index}}\n          </a>\n        {{/if}}\n      {{/each}}\n\n    {{/for}}\n\n    {{#each items}}\n      {{#if selected '==' true}}\n        <span class=\"selected-rating\">\n          {{lang 'faceted_search.stars' num=param_value}}\n        </span>\n      {{/if}}\n    {{/each}}\n  </li>\n</ul>\n"}