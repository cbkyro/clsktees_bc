{"components/products/product-single-images":"<div class=\"product-images-container\" data-product-images>\n  <div class=\"product-slides-container\">\n    <div\n      class=\"product-slides-wrap\"\n      data-adaptive-height=\"{{theme_settings.product-single-image-adaptive-height}}\"\n    >\n      {{#each images}}\n        <a\n          class=\"product-image\"\n          href=\"{{getImage this 'zoom'}}\"\n          data-product-image\n        >\n          <img data-lazy=\"{{getImage this 'product'}}\" alt=\"{{alt}}\">\n        </a>\n      {{/each}}\n    </div>\n  </div>\n  <ul class=\"product-images-pagination {{#if images.length '==' 1}}hidden{{/if}}\">\n    {{#each images}}\n      <li class=\"pagination-item\">\n        <a\n          href=\"#\"\n          data-slide-to=\"{{@index}}\"\n          style=\"background-image: url('{{getImage this 'thumb'}}');\"\n        >\n          <img\n            class=\"sr-only\"\n            src=\"{{getImage this 'thumb'}}\"\n            alt=\"{{this.alt}}\"\n          >\n        </a>\n      </li>\n    {{/each}}\n  </ul>\n</div>\n"}