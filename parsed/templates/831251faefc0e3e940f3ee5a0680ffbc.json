{"components/products/success-modal":"<div class=\"success-wrapper\">\n  {{> components/common/modal-close}}\n  <div class=\"site-alerts modal-alerts\" data-modal-alerts>\n  </div>\n  <div class=\"success-content\">\n    <div class=\"success-item-wrap\">\n    {{#with cart.added_item}}\n      <div class=\"success-item-thumb\" style=\"background-image: url('{{getImage image 'thumb' (cdn ../../theme_settings.default_image)}}')\"></div>\n      <img class=\"sr-only\" src=\"{{getImage image 'thumb' (cdn ../theme_settings.default_image)}}\" alt=\"{{image.alt}}\" />\n      <div class=\"success-item-description\">\n        <div class=\"success-item-brand\">{{brand.name}}</div>\n        <div class=\"success-item-title\">{{name}}</div>\n        {{#if options}}\n          <div class=\"success-product-option\">\n            {{#each options}}\n              <span class=\"success-option-label\">{{value}}</span>\n            {{/each}}\n          </div>\n        {{/if}}\n        {{#or customer (unless theme_settings.restrict_to_login)}}\n          <span class=\"success-item-price\">\n            {{price.formatted}} &times; {{quantity}}\n          </span>\n        {{else}}\n          {{> components/common/login-for-pricing}}\n        {{/or}}\n      </div>\n    {{/with}}\n  </div>\n  <h4 class=\"success-modal-title\">\n    <span>{{lang 'mini_cart.success_modal.subtotal'}}</span>\n    {{#or customer (unless theme_settings.restrict_to_login)}}\n      <span class=\"success-modal-subtotal\">{{cart.sub_total.formatted}}</span>\n    {{else}}\n      {{> components/common/login-for-pricing}}\n    {{/or}}\n  </h4>\n  <div class=\"success-modal-actions\">\n    <a class=\"button button-secondary\" href=\"{{urls.cart}}\">{{lang 'mini_cart.view_cart'}}</a>\n    <a class=\"button\" href=\"{{urls.checkout.single_address}}\">{{lang 'mini_cart.checkout'}}</a>\n  </div>\n  <div class=\"success-modal-additional-checkout\">\n    {{#each cart.additional_checkout_buttons}}\n      {{{this}}}\n    {{/each}}\n  </div>\n  </div>\n</div>\n","components/common/login-for-pricing":"<span class=\"login-for-pricing\">\n  <a class=\"login-for-pricing-link link\" href=\"{{urls.auth.login}}\">\n    {{lang 'common.login_for_pricing'}}\n  </a>\n</span>\n","components/common/modal-close":"<a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'cart.modal_close'}}\">\n  <span aria-hidden=\"true\">\n    <svg class=\"icon icon-close\"><use xlink:href=\"#icon-close\" /></svg>\n  </span>\n</a>\n"}