{"components/cart/totals":"<div class=\"cart-totals-column discount-fields\">\n  {{> components/cart/coupon-codes}}\n  {{#if settings.gift_certificates_enabled}}\n    {{> components/cart/gift-certificates}}\n  {{/if}}\n</div>\n<div class=\"cart-totals-column totals\">\n  {{#if cart.discount.formatted}}\n    <div class=\"cart-sub-total cart-totals-item\">\n      <span class=\"cart-total-label\">{{lang 'cart.discount'}}</span>\n      <span class=\"cart-total-item\">{{cart.discount.formatted}}</span>\n    </div>\n  {{/if}}\n  <div class=\"cart-sub-total cart-totals-item\">\n    <span class=\"cart-total-label\">{{lang 'cart.sub_total'}}:</span>\n    {{#or customer (unless theme_settings.restrict_to_login)}}\n      <span class=\"cart-total-item\">{{cart.sub_total.formatted}}</span>\n    {{else}}\n      {{> components/common/login-for-pricing}}\n    {{/or}}\n  </div>\n  {{> components/cart/shipping-calculator cart.shipping_handling}}\n  {{#if cart.coupons}}\n    <div class=\"cart-totals-item\">\n      <ul class=\"coupons-active\">\n        {{#each cart.coupons}}\n          <li class=\"discount-active\">\n            <span class=\"cart-total-label\">\n              {{lang 'cart.discount'}}\n              <br>\n              {{lang 'cart.coupon_codes.label' code=code}}\n            </span>\n            <span class=\"cart-total-item\">{{discount.formatted}}</span>\n            <a class=\"link\" href=\"{{remove_url}}\">\n              {{lang 'cart.coupon_codes.remove'}}\n            </a>\n          </li>\n        {{/each}}\n      </ul>\n    </div>\n  {{/if}}\n  {{#if cart.gift_certificates}}\n    <div class=\"cart-totals-item\">\n      <ul>\n        {{#each cart.gift_certificates}}\n          <li class=\"discount-active\">\n            <span class=\"cart-total-label\">\n              {{lang 'cart.gift_certificates.label' code=code}}\n              <br>\n              {{lang 'cart.gift_certificates.remaining' amount_remaining=remaining.formatted}}\n            </span>\n            <span class=\"cart-total-item\">{{used.formatted}}</span>\n            <a class=\"link\" href=\"{{remove_url}}\">\n              {{lang 'cart.gift_certificates.remove'}}\n            </a>\n          </li>\n        {{/each}}\n      </ul>\n    </div>\n  {{/if}}\n  {{#if cart.gift_wrapping_cost.value '!=' 0}}\n    <div class=\"cart-totals-item\">\n      <span class=\"cart-total-label\">{{lang 'cart.gift_wrapping.title'}}</span>\n      <span class=\"cart-total-item\">{{cart.gift_wrapping_cost.formatted}}</span>\n    </div>\n  {{/if}}\n  {{#each cart.taxes}}\n    <div class=\"cart-totals-item\">\n      <span class=\"cart-total-label\">{{name}}:</span>\n      <span class=\"cart-total-item\">{{cost.formatted}}</span>\n    </div>\n  {{/each}}\n  <div class=\"cart-grand-total cart-totals-item\">\n    <span class=\"cart-total-label\">{{lang 'cart.grand_total'}}:</span>\n    {{#or customer (unless theme_settings.restrict_to_login)}}\n      <span class=\"cart-total-item grand-total\">\n        {{cart.grand_total.formatted}}\n      </span>\n    {{else}}\n      {{> components/common/login-for-pricing}}\n    {{/or}}\n  </div>\n  <div class=\"cart-actions\">\n    <a class=\"button button-secondary button-wide\" href=\"{{urls.home}}\">\n      {{lang 'cart.keep_shopping'}}\n    </a>\n    <a\n      class=\"button button-primary button-wide button-checkout\"\n      href=\"{{urls.checkout.single_address}}\"\n    >\n      {{lang 'cart.checkout'}}\n    </a>\n    {{#if cart.show_multiple_address_shipping}}\n      <a class=\"checkout-multiple\" href=\"{{urls.checkout.multiple_address}}\">\n        {{lang 'cart.checkout_multiple'}}\n      </a>\n    {{/if}}\n  </div>\n  {{#if cart.additional_checkout_buttons.length}}\n    <div class=\"additional-checkout-buttons\">\n      {{#each cart.additional_checkout_buttons}}\n        {{{this}}}\n      {{/each}}\n    </div>\n  {{/if}}\n</div>\n","components/cart/coupon-codes":"{{inject 'couponCodeEmptyInput' (lang 'cart.coupon_codes.error')}}\n\n<div class=\"cart-totals-item coupon-code\" data-coupon-codes>\n  <form class=\"form form-coupon\" method=\"post\" action=\"{{urls.cart}}\" data-coupon-code-form>\n    <div class=\"input-group\">\n      <div class=\"form-field input-group-item\">\n        <label class=\"form-label\" for=\"couponcode\">\n          <span class=\"form-label-text sr-only\">{{lang 'cart.coupon_codes.enter_code'}}</span>\n          <input class=\"form-input\" id=\"couponcode\" type=\"text\" name=\"couponcode\" value=\"\" placeholder=\"{{lang 'cart.coupon_codes.enter_code'}}\" data-coupon-code-input>\n        </label>\n      </div>\n      <div class=\"form-actions input-group-item\">\n        <input class=\"button button-tertiary\" type=\"submit\" value=\"{{lang 'cart.coupon_codes.submit'}}\">\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n      </div>\n    </div>\n  </form>\n</div>\n","components/cart/gift-certificates":"<div class=\"cart-totals-item gift-certificates\">\n  {{inject 'giftCertificateInputEmpty' (lang 'cart.gift_certificates.error')}}\n\n  <div data-gift-certificates>\n    <span class=\"cart-total-label\">\n      {{lang 'cart.gift_certificates.gift_certificate'}}:\n    </span>\n\n    <span class=\"link\" data-gift-certificate-toggle>\n      {{lang 'cart.gift_certificates.add_code'}}\n    </span>\n\n    <div class=\"modal-content-wrap\" id=\"certificate-modal\">\n      {{> components/common/modal-close}}\n      <header class=\"modal-header\">\n        <h2 class=\"title\">{{lang 'cart.gift_certificates.title'}}</h2>\n        <p class=\"subtitle\">{{lang 'cart.gift_certificates.subtitle'}}</p>\n      </header>\n      <form\n        class=\"form form-gift-certificate\"\n        method=\"post\"\n        action=\"{{urls.cart}}\"\n        data-gift-certificate-form\n      >\n        <label class=\"sr-only form-label\" for=\"couponcode\">\n          {{lang 'cart.gift_certificates.enter_code'}}\n        </label>\n        <div class=\"input-group\">\n          <div class=\"form-field input-group-item\">\n            <input\n              class=\"form-input\"\n              id=\"couponcode\"\n              type=\"text\"\n              name=\"certcode\"\n              value=\"\"\n              placeholder=\"{{lang 'cart.gift_certificates.enter_code'}}\"\n              data-gift-certificate-input\n            >\n          </div>\n          <div class=\"form-field input-group-item\">\n            <button\n              class=\"button button-tertiary button-progress spinner\"\n              type=\"submit\"\n            >\n              <span class=\"button-text\">\n                {{lang 'cart.gift_certificates.submit'}}\n                <span class=\"button-text\">\n            </button>\n          </div>\n        </div>\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n      </form>\n    </div>\n  </div>\n</div>\n","components/common/login-for-pricing":"<span class=\"login-for-pricing\">\n  <a class=\"login-for-pricing-link link\" href=\"{{urls.auth.login}}\">\n    {{lang 'common.login_for_pricing'}}\n  </a>\n</span>\n","components/cart/shipping-calculator":"{{#if show_estimator}}\n  <div class=\"cart-totals-item\">\n    <span class=\"cart-total-label\">\n      {{lang 'cart.shipping_calculator.title'}}:\n    </span>\n    {{#if shipping_cost}}\n      <span class=\"cart-total-item\">{{shipping_cost.formatted}}</span>\n    {{/if}}\n\n    {{#if shipping_cost \"===\" 0}}\n      {{#if provider}}\n        <span class=\"link\" data-shipping-calculator-toggle>\n          {{provider.description}}\n        </span>\n      {{/if}}\n    {{else}}\n      <span\n        class=\"link\"\n        data-shipping-calculator-toggle\n        data-change-text=\"{{lang 'cart.shipping_calculator.change_info'}}\"\n      >\n        <span>{{lang 'cart.shipping_calculator.calculate_shipping'}}</span>\n      </span>\n    {{/if}}\n\n    <div class=\"modal-content-wrap\" id=\"shipping-modal\">\n      {{> components/common/modal-close}}\n      <header class=\"modal-header\">\n        <h2 class=\"title\">{{lang 'cart.shipping_calculator.title'}}</h2>\n        <p class=\"subtitle\">{{lang 'cart.shipping_calculator.subtitle'}}</p>\n      </header>\n      <div class=\"shipping-calculator\" data-shipping-calculator>\n        <form class=\"form\" data-shipping-calculator-address>\n          <div class=\"form-field\">\n            <label class=\"form-label\" for=\"shipping-country\">\n              <span class=\"sr-only form-label-text\">\n                {{lang 'cart.shipping_calculator.select_a_country'}}\n              </span>\n              <div class=\"form-select-wrapper\">\n                <select\n                  class=\"form-input form-select form-input-short\"\n                  id=\"shipping-country\"\n                  name=\"shipping-country\"\n                  data-field-type=\"Country\"\n                >\n                  <option>\n                    {{lang 'cart.shipping_calculator.select_a_country'}}\n                  </option>\n                  {{#each countries}}\n                    <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                      {{name}}\n                    </option>\n                  {{/each}}\n                </select>\n              </div>\n            </label>\n          </div>\n\n          <div class=\"form-field\">\n            <label class=\"form-label\" for=\"shipping-state\">\n              <span class=\"sr-only form-label-text\">\n                {{lang 'cart.shipping_calculator.select_a_state'}}\n              </span>\n              <div class=\"form-select-wrapper shipping-state-wrapper\">\n                <select\n                  class=\"form-input form-select form-input-short\"\n                  id=\"shipping-state\"\n                  name=\"shipping-state\"\n                  data-field-type=\"State\"\n                >\n                  <option>\n                    {{lang 'cart.shipping_calculator.select_a_state'}}\n                  </option>\n                  {{#each states}}\n                    <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                      {{name}}\n                    </option>\n                  {{/each}}\n                </select>\n              </div>\n            </label>\n          </div>\n\n          <div class=\"form-field shipping-zip\">\n            <label class=\"form-label\" for=\"shipping-zip\">\n              <span class=\"sr-only form-label-text\">\n                {{lang 'cart.shipping_calculator.zip_postal_code'}}\n              </span>\n              <input\n                class=\"form-input form-input-short\"\n                type=\"text\"\n                id=\"shipping-zip\"\n                name=\"shipping-zip\"\n                value=\"{{selected_zip}}\"\n                placeholder=\"{{lang 'cart.shipping_calculator.zip_postal_code'}}\"\n              >\n            </label>\n          </div>\n\n          <div class=\"form-actions get-shipping-quotes\">\n            <button class=\"button button-short button-secondary button-progress spinner\">\n              <span class=\"button-text\">\n                {{lang 'cart.shipping_calculator.estimate_shipping'}}\n              </span>\n            </button>\n          </div>\n        </form>\n\n        <div class=\"shipping-quotes\" data-shipping-quotes></div>\n      </div>\n    </div>\n\n  </div>\n{{/if}}\n","components/common/modal-close":"<a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'cart.modal_close'}}\">\n  <span aria-hidden=\"true\">\n    <svg class=\"icon icon-close\"><use xlink:href=\"#icon-close\" /></svg>\n  </span>\n</a>\n"}