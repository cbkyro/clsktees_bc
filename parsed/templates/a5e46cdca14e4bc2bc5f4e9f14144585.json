{"components/wishlist/wishlist-buttons":"<div\n  class=\"product-wishlist {{#if customer.wishlists}}dropdown{{/if}}\"\n  {{#if customer.wishlists}}data-dropdown{{/if}}\n  data-product-wishlist\n>\n  {{#if customer.wishlists}}\n    <div\n      class=\"\n        dropdown-panel\n        wishlist-dropdown\n        {{#unless product.can_purchase}}\n          no-add-to-cart\n        {{/unless}}\n      \"\n      data-dropdown-panel\n    >\n      {{> components/wishlist/wishlist-lists}}\n    </div>\n    <a\n      class=\"\n        button\n        button-secondary\n        button-wide\n        dropdown-toggle\n        button-progress\n        spinner\n      \"\n      href=\"#\"\n      data-dropdown-toggle\n      data-product-wishlist\n    >\n      <span class=\"button-text\">\n        {{lang 'product.add_to_wishlist'}}\n      </span>\n    </a>\n\n  {{else}}\n    <a\n      href=\"{{product.add_to_wishlist_url}}\"\n      class=\"button button-secondary button-wide\"\n    >\n      <span class=\"button-text\">\n        {{lang 'product.add_to_wishlist'}}\n      </span>\n    </a>\n  {{/if}}\n</div>\n","components/wishlist/wishlist-lists":"<div class=\"override-dropdown\" data-dropdown-close>\n {{> components/common/modal-close}}\n</div>\n<div>\n  <span class=\"wishlist-title\">\n    {{lang \"product.choose_wishlist\"}}\n  </span>\n  <ul class=\"wishlist\">\n    {{#if customer.wishlists}}\n      {{#each customer.wishlists}}\n        <li class=\"wishlist-item\">\n          <a\n            href=\"{{../product.add_to_wishlist_url}}&wishlistid={{id}}\"\n            data-wishlist=\"{{view_url}}\"\n            data-name=\"{{name}}\"\n          >\n            {{name}}\n          </a>\n        </li>\n      {{/each}}\n    {{/if}}\n    <li class=\"wishlist-item\">\n      <a class=\"create-wishlist\" href=\"#create-wishlist\" data-create-wishlist>\n        <span class=\"create-wishlist-plus\"></span>\n        <span class=\"create-wishlist-text\">\n          {{lang \"product.create_wishlist\"}}\n        </span>\n      </a>\n    </li>\n  </ul>\n</div>\n","components/common/modal-close":"<a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'cart.modal_close'}}\">\n  <span aria-hidden=\"true\">\n    <svg class=\"icon icon-close\"><use xlink:href=\"#icon-close\" /></svg>\n  </span>\n</a>\n"}