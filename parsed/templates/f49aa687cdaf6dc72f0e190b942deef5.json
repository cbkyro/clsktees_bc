{"components/products/add-to-cart-form":"<form\n  method=\"post\"\n  id=\"product-add-cart\"\n  action=\"{{product.cart_url}}\"\n  enctype=\"multipart/form-data\"\n  data-cart-item-add\n  data-product-options-count=\"{{product.options.length}}\"\n  data-validated-form\n  {{#if customer}}data-is-customer{{/if}}\n>\n  <input type=\"hidden\" name=\"action\" value=\"add\">\n  <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\" data-product-id/>\n\n  <div class=\"product-options-container\" data-product-option-change>\n    {{#if theme_settings.bulk_pricing_location '===' 'product-details-top'}}\n      {{> components/products/bulk-pricing}}\n    {{/if}}\n\n    {{#if product.customizations.length}}\n      {{#each product.customizations}}\n        {{{dynamicComponent 'core/forms/customizations'}}}\n      {{/each}}\n    {{/if}}\n\n    {{#if product.options.length}}\n      {{#each product.options}}\n        {{{dynamicComponent 'core/forms/options'}}}\n      {{/each}}\n    {{/if}}\n  </div>\n\n  {{#if product.event_date}}\n    {{> core/products/event}}\n  {{/if}}\n\n  <div\n    class=\"\n      product-stock\n      {{#unless product.show_quantity_input}}\n        default-layout\n      {{/unless}}\n    \"\n  >\n    <span\n      class=\"{{#unless product.stock_level}}product-details-hidden{{/unless}}\"\n      data-product-stock\n    >\n      {{lang 'product.current_stock'}}\n    </span>\n    <span\n      class=\"{{#unless product.stock_level}}product-details-hidden{{/unless}}\"\n      data-product-stock\n      data-product-stock-level\n    >\n      {{#if product.stock_level}}\n        {{product.stock_level}}\n      {{else}}\n        0\n      {{/if}}\n    </span>\n\n    {{#if product.out_of_stock}}\n      <span class=\"stock-message\">\n      {{#if product.out_of_stock_message}}\n        {{product.out_of_stock_message}}\n      {{else}}\n        {{lang 'product.sold_out'}}\n      {{/if}}\n    </span>\n    {{/if}}\n  </div>\n\n  {{#if product.show_quantity_input}}\n    <div class=\"product-quantity-container\">\n      <div class=\"form-field\">\n        <label class=\"form-label\">\n          <span class=\"form-label-text product-option-title\">\n            {{lang 'product.quantity'}}\n          </span>\n          {{> components/common/quantity product}}\n        </label>\n      </div>\n    </div>\n  {{/if}}\n\n  <div class=\"product-actions\">\n    {{#if theme_settings.bulk_pricing_location '===' 'product-details-bottom'}}\n      {{> components/products/bulk-pricing}}\n    {{/if}}\n    {{#or customer (unless theme_settings.restrict_to_login)}}\n      {{#if product.can_purchase}}\n        <button\n          type=\"submit\"\n          class=\"button button-primary button-wide add-to-cart button-progress spinner\"\n          data-progress-complete=\"{{lang 'product.added'}}\"\n          data-product-title=\"{{product.title}}\"\n          data-button-purchase\n        >\n          <span class=\"button-text\">\n            {{#if product.pre_order}}\n              {{lang 'product.pre_order'}}\n            {{else}}\n              {{lang 'product.add_to_cart'}}\n            {{/if}}\n          </span>\n        </button>\n      {{else}}\n        {{#if product.out_of_stock}}\n          <div class=\"button button-primary button-wide add-to-cart button-disabled\">\n            {{lang 'product.sold_out'}}\n          </div>\n        {{/if}}\n      {{/if}}\n    {{/or}}\n\n    {{#if settings.show_wishlist}}\n      {{> components/wishlist/wishlist-buttons}}\n    {{/if}}\n\n    <div class=\"modal-content-wrap\" id=\"success-modal\">\n      {{> components/products/success-modal}}\n    </div>\n\n    <div class=\"product-message-area\" data-product-message></div>\n  </div>\n</form>\n","components/products/bulk-pricing":"{{#if product.bulk_discount_rates.length}}\n  <div class=\"bulk-pricing-block\">\n    <h1 class=\"product-details-heading\">\n      {{lang 'product.bulk_pricing.title'}}\n    </h1>\n    <ul class=\"bulk-pricing-list\">\n      <li class=\"bulk-pricing-item bulk-pricing-item-title\">\n        <span class=\"bulk-pricing-range\">\n          {{lang 'product.bulk_pricing.qty'}}\n        </span>\n        <span class=\"bulk-pricing-value\">\n          {{#each (limit product.bulk_discount_rates 1)}}\n            {{#if type '===' 'fixed'}}\n              {{lang 'product.bulk_pricing.price_per_unit'}}\n            {{else}}\n              {{lang 'product.bulk_pricing.discount'}}\n            {{/if}}\n          {{/each}}\n        </span>\n      </li>\n      {{#each product.bulk_discount_rates}}\n        <li class=\"bulk-pricing-item\">\n          <span class=\"bulk-pricing-range\">\n            {{lang 'product.bulk_pricing.range' min=min max=max}}\n          </span>\n          <span class=\"bulk-pricing-value\">\n            {{lang 'product.bulk_pricing.discount_formatted' discount=discount.formatted}}\n          </span>\n        </li>\n      {{/each}}\n    </ul>\n  </div>\n{{/if}}\n","core/products/event":"<div class=\"form-field form-field-date form-required\">\n  <span class=\"form-label\">\n\n    <span class=\"form-field-title\">\n      {{product.event_date.name}}\n      <span class=\"required-text\">{{lang 'common.required'}}</span>\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select class=\"form-input form-select\" name=\"EventDate[Yr]\" required>\n          <option disabled selected>{{lang 'forms.date.year'}}</option>\n          {{#if product.event_date.earliest_year '==' product.event_date.latest_year}}\n            <option value=\"{{product.event_date.earliest_year}}\">{{product.event_date.earliest_year}}</option>\n          {{else}}\n            {{#for product.event_date.earliest_year product.event_date.latest_year}}\n              <option value=\"{{$index}}\">{{$index}}</option>\n            {{/for}}\n          {{/if}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-month\">\n        <select class=\"form-input form-select\" name=\"EventDate[Mth]\" required>\n          <option disabled selected>{{lang 'forms.date.month'}}</option>\n          {{#for 1 12}}\n            <option value=\"{{$index}}\">{{getShortMonth $index}}</option>\n          {{/for}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-day\">\n        <select class=\"form-input form-select\" name=\"EventDate[Day]\" required>\n          <option disabled selected>{{lang 'forms.date.day'}}</option>\n          {{#for 1 31}}\n            <option value=\"{{$index}}\">{{$index}}</option>\n          {{/for}}\n        </select>\n      </span>\n    </span>\n\n  </span>\n\n  {{#if product.event_date.date_start}}\n    {{#if product.event_date.date_end}}\n      <span class=\"product-event-bounds\">{{lang 'forms.date.bounds' start_date=product.event_date.date_start end_date=product.event_date.date_end}}</span>\n    {{/if}}\n  {{/if}}\n</div>\n","components/products/success-modal":"<div class=\"success-wrapper\">\n  {{> components/common/modal-close}}\n  <div class=\"site-alerts modal-alerts\" data-modal-alerts>\n  </div>\n  <div class=\"success-content\">\n    <div class=\"success-item-wrap\">\n    {{#with cart.added_item}}\n      <div class=\"success-item-thumb\" style=\"background-image: url('{{getImage image 'thumb' (cdn ../../theme_settings.default_image)}}')\"></div>\n      <img class=\"sr-only\" src=\"{{getImage image 'thumb' (cdn ../theme_settings.default_image)}}\" alt=\"{{image.alt}}\" />\n      <div class=\"success-item-description\">\n        <div class=\"success-item-brand\">{{brand.name}}</div>\n        <div class=\"success-item-title\">{{name}}</div>\n        {{#if options}}\n          <div class=\"success-product-option\">\n            {{#each options}}\n              <span class=\"success-option-label\">{{value}}</span>\n            {{/each}}\n          </div>\n        {{/if}}\n        {{#or customer (unless theme_settings.restrict_to_login)}}\n          <span class=\"success-item-price\">\n            {{price.formatted}} &times; {{quantity}}\n          </span>\n        {{else}}\n          {{> components/common/login-for-pricing}}\n        {{/or}}\n      </div>\n    {{/with}}\n  </div>\n  <h4 class=\"success-modal-title\">\n    <span>{{lang 'mini_cart.success_modal.subtotal'}}</span>\n    {{#or customer (unless theme_settings.restrict_to_login)}}\n      <span class=\"success-modal-subtotal\">{{cart.sub_total.formatted}}</span>\n    {{else}}\n      {{> components/common/login-for-pricing}}\n    {{/or}}\n  </h4>\n  <div class=\"success-modal-actions\">\n    <a class=\"button button-secondary\" href=\"{{urls.cart}}\">{{lang 'mini_cart.view_cart'}}</a>\n    <a class=\"button\" href=\"{{urls.checkout.single_address}}\">{{lang 'mini_cart.checkout'}}</a>\n  </div>\n  <div class=\"success-modal-additional-checkout\">\n    {{#each cart.additional_checkout_buttons}}\n      {{{this}}}\n    {{/each}}\n  </div>\n  </div>\n</div>\n","components/common/quantity":"{{!\n  Quantity Widget with Increment/Decrement controls.\n\n  @param product\n         cart.item or product object\n}}\n\n<div class=\"quantity-control\" data-quantity-control=\"{{id}}\">\n  <input\n    class=\"form-input\"\n    type=\"number\"\n    name=\"qty[]\"\n    pattern=\"[0-9]*\"\n    value=\"{{#if quantity}}{{quantity}}{{else}}{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}1{{/if}}{{/if}}\"\n    min=\"{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}1{{/if}}\"\n    {{#if max_purchase_quantity}}max=\"{{max_purchase_quantity}}\"{{/if}}\n    data-quantity-control-input\n  >\n  <button class=\"button-control-action\" data-quantity-control-action=\"increment\">\n    <span class=\"button-control-symbol\">\n      &#43;\n    </span>\n  </button>\n  <button class=\"button-control-action\" data-quantity-control-action=\"decrement\">\n    <span class=\"button-control-symbol\">\n      &#8211;\n    </span>\n  </button>\n</div>\n","components/wishlist/wishlist-buttons":"<div\n  class=\"product-wishlist {{#if customer.wishlists}}dropdown{{/if}}\"\n  {{#if customer.wishlists}}data-dropdown{{/if}}\n  data-product-wishlist\n>\n  {{#if customer.wishlists}}\n    <div\n      class=\"\n        dropdown-panel\n        wishlist-dropdown\n        {{#unless product.can_purchase}}\n          no-add-to-cart\n        {{/unless}}\n      \"\n      data-dropdown-panel\n    >\n      {{> components/wishlist/wishlist-lists}}\n    </div>\n    <a\n      class=\"\n        button\n        button-secondary\n        button-wide\n        dropdown-toggle\n        button-progress\n        spinner\n      \"\n      href=\"#\"\n      data-dropdown-toggle\n      data-product-wishlist\n    >\n      <span class=\"button-text\">\n        {{lang 'product.add_to_wishlist'}}\n      </span>\n    </a>\n\n  {{else}}\n    <a\n      href=\"{{product.add_to_wishlist_url}}\"\n      class=\"button button-secondary button-wide\"\n    >\n      <span class=\"button-text\">\n        {{lang 'product.add_to_wishlist'}}\n      </span>\n    </a>\n  {{/if}}\n</div>\n","core/forms/customizations/checkbox":"\n<div class=\"form-field form-field-customizations form-field-checkbox{{#if required}} form-required{{/if}}\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n  <label class=\"form-label\">\n    <span class=\"form-field-control\">\n      <input class=\"form-input form-checkbox\"\n        id=\"product-field-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        type=\"checkbox\"\n        {{#if value}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"\n      >\n      <span class=\"form-label-text product-customization-text\">\n        {{display_name}}\n      </span>\n    </span>\n  </label>\n</div>\n","core/forms/customizations/file":"\n<div class=\"form-field form-field-customizations form-field-file {{#if required}}form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-file-wrapper\" data-button-label=\"{{lang 'core.account.products.upload'}}\">\n        <input\n          class=\"form-input form-file\"\n          id=\"product-fields-{{id}}\"\n          name=\"ProductFields[{{this.id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n      </span>\n    </span>\n  </label>\n\n  {{#if file_types '!==' ''}}\n    <span class=\"form-file-types\">{{lang 'core.account.products.filetypes' file_types=file_types}}</span>\n  {{/if}}\n\n  <span class=\"form-file-size\">{{lang 'core.account.products.max_filesize' bytes=file_size}}</span>\n</div>\n","core/forms/customizations/select":"\n<div class=\"form-field form-field-customizations form-field-select{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper\">\n        <select\n          class=\"form-input input-select\"\n          id=\"product-fields-{{id}}\"\n          name=\"ProductFields[{{this.id}}]\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n          <option value=\"\" {{#any values selected=true}}{{else}}selected{{/any}} disabled>{{lang 'product.pick_one'}}</option>\n          {{#each values}}\n            <option value=\"{{this}}\" {{#if selected}}selected{{/if}}>{{this}}</option>\n          {{/each}}\n        </select>\n      </span>\n    </span>\n  </label>\n</div>\n","core/forms/customizations/text":"\n<div class=\"form-field form-field-customizations form-field-text{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\" for=\"product-fields-{{id}}\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"product-fields-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        value=\"{{value}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/customizations/textarea":"\n<div class=\"form-field form-field-customizations form-field-textarea{{#if required}} form-required{{/if}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}:\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <textarea\n        class=\"form-input\"\n        id=\"product-fields-{{id}}\"\n        name=\"ProductFields[{{this.id}}]\"\n        rows=\"4\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">{{prefill}}</textarea>\n    </span>\n  </label>\n</div>\n","core/forms/options/date":"<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-date\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"date\"\n>\n  <span class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}\n        <span class=\"required-text\">{{lang 'common.required'}}</span>\n      {{/if}}\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select\n          class=\"form-input form-select\"\n          name=\"attribute[{{id}}][year]\"\n          {{#if required}}\n            required\n          {{/if}}\n          aria-required=\"{{required}}\"\n        >\n          <option selected disabled>\n            {{lang 'forms.date.year'}}\n          </option>\n          {{#if earliest_year '===' latest_year}}\n            <option value=\"{{earliest_year}}\" selected=\"selected\">\n              {{earliest_year}}\n            </option>\n          {{else}}\n            {{#for earliest_year latest_year}}\n              <option\n                value=\"{{$index}}\"\n                {{#if ../selected_date.year '==' $index}}\n                  selected=\"selected\"\n                {{/if}}\n              >\n                {{$index}}\n              </option>\n            {{/for}}\n          {{/if}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-month\">\n        <select\n          class=\"form-input form-select\"\n          name=\"attribute[{{id}}][month]\"\n          {{#if required}}\n            required\n          {{/if}}\n          aria-required=\"{{required}}\"\n        >\n          <option selected disabled>{{lang 'forms.date.month'}}</option>\n          {{#for 1 12}}\n            <option\n              value=\"{{$index}}\"\n              {{#if ../selected_date.month '==' $index}}\n                selected=\"selected\"\n              {{/if}}\n            >\n              {{getShortMonth $index}}\n            </option>\n          {{/for}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-day\">\n        <select\n          class=\"form-input form-select\"\n          name=\"attribute[{{id}}][day]\"\n          {{#if required}}\n            required\n          {{/if}}\n          aria-required=\"{{required}}\"\n        >\n          <option selected disabled>{{lang 'forms.date.day'}}</option>\n          {{#for 1 31}}\n            <option\n              value=\"{{$index}}\"\n              {{#if ../selected_date.day '==' $index}}\n                selected=\"selected\"\n              {{/if}}\n            >\n              {{$index}}\n            </option>\n          {{/for}}\n        </select>\n        </span>\n    </span>\n\n  </span>\n</div>\n","core/forms/options/input-checkbox":"<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-checkbox\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"input-checkbox\"\n>\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}\n      <span class=\"required-text\">{{lang 'common.required'}}</span>\n    {{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    <label class=\"form-label {{class}}\">\n      <input\n        class=\"form-input form-checkbox\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"checkbox\"\n        value=\"{{value}}\"\n        {{#if checked}}\n          checked\n          data-default\n        {{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"\n      >\n      <span class=\"form-label-text\">{{label}}</span>\n    </label>\n  </div>\n</div>\n","core/forms/options/input-file":"<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-file\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"input-file\"\n>\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}\n        <span class=\"required-text\">{{lang 'common.required'}}</span>\n      {{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span\n        class=\"form-file-wrapper\"\n        data-button-label=\"{{lang 'core.account.products.upload'}}\"\n      >\n        <input\n          class=\"form-input form-file\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\"\n        >\n      </span>\n    </span>\n  </label>\n\n  {{#if file_types '!==' ''}}\n    <span class=\"form-file-types\">\n      {{lang 'core.account.products.filetypes' file_types=file_types}}\n    </span>\n  {{/if}}\n\n  <span class=\"form-file-size\">\n    {{lang 'core.account.products.max_filesize' bytes=file_size}}\n  </span>\n</div>\n","core/forms/options/input-numbers":"<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-number\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"input-number\"\n>\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}\n        <span class=\"required-text\">{{lang 'common.required'}}</span>\n      {{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input\n        class=\"form-input\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"number\"\n        value=\"{{prefill}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"\n      >\n    </span>\n  </label>\n</div>\n","core/forms/options/input-text":"<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-text\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"input-text\"\n>\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}\n        <span class=\"required-text\">{{lang 'common.required'}}</span>\n      {{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"text\"\n        value=\"{{prefill}}\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"\n      >\n    </span>\n  </label>\n</div>\n","core/forms/options/product-list":"<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-product-list\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"product-list\"\n>\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}\n      <span class=\"required-text\">{{lang 'common.required'}}</span>\n    {{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    {{#unless values.0.image}}\n      {{!-- same as radio buttons if no image set --}}\n      {{#each values}}\n        <label class=\"form-label\" data-product-attribute-value=\"{{id}}\">\n          <input\n            class=\"form-input form-radio\"\n            name=\"attribute[{{../id}}]\"\n            type=\"radio\"\n            value=\"{{id}}\"\n            {{#if selected}}\n              checked\n              data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\"\n          >\n          <span class=\"form-label-text\">{{label}}</span>\n        </label>\n      {{/each}}\n      {{#unless required}}\n        <label class=\"form-label\">\n          <input\n            class=\"form-input form-radio\"\n            name=\"attribute[{{id}}]\"\n            type=\"radio\"\n            value=\"0\"\n            {{#if prefill '==' 0}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\"\n          >\n          <span class=\"form-label-text\">{{lang 'core.product.none'}}</span>\n        </label>\n      {{/unless}}\n    {{else}}\n      {{#each values}}\n        <label\n          class=\"form-label product-picklist-item\"\n          data-product-attribute-value=\"{{id}}\"\n        >\n          <input\n            class=\"form-input form-radio product-picklist-radio\"\n            name=\"attribute[{{../id}}]\"\n            type=\"radio\"\n            value=\"{{id}}\"\n            {{#if selected}}\n              checked\n              data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\"\n          >\n          <span class=\"product-picklist-item-body\">\n            <span class=\"product-picklist-image-wrap\">\n              <img\n                class=\"product-picklist-image\"\n                src=\"{{getImage image 'core-product-picklist' (cdn ../theme_settings.default_image)}}\"\n                alt=\"{{image.alt}}\"\n              >\n            </span>\n            <span class=\"product-picklist-label-text form-label-text\">\n              {{label}}\n            </span>\n          </span>\n        </label>\n      {{/each}}\n      {{#unless required}}\n        <label class=\"form-label product-picklist-item\">\n          <input\n            class=\"form-input form-radio product-picklist-radio\"\n            name=\"attribute[{{id}}]\"\n            type=\"radio\"\n            value=\"0\"\n            {{#if prefill '==' 0}}checked{{/if}}\n          >\n          <span class=\"product-picklist-item-body picklist-none\">\n            <span class=\"product-picklist-label-text form-label-text\">\n              {{lang 'product.none'}}\n            </span>\n          </span>\n        </label>\n      {{/unless}}\n    {{/unless}}\n  </div>\n</div>\n","core/forms/options/set-radio":"<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-radio\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"set-radio\"\n>\n  <div class=\"form-field-title product-option-title\">\n    {{display_name}}\n    {{#if required}}\n      <span class=\"required-text\">{{lang 'common.required'}}</span>\n    {{/if}}\n  </div>\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"form-label\" data-product-attribute-value=\"{{id}}\">\n        <input\n          class=\"form-input form-radio\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"{{id}}\"\n          {{#if selected}}\n            checked\n            data-default\n          {{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\"\n        >\n        <span class=\"form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n    {{#unless required}}\n      <label class=\"form-label\">\n        <input\n          class=\"form-input form-radio\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"0\"\n          {{#if prefill '==' 0}}checked{{/if}}\n          aria-required=\"{{required}}\"\n        >\n        <span class=\"form-label-text\">{{lang 'core.product.none'}}</span>\n      </label>\n    {{/unless}}\n  </div>\n</div>\n","core/forms/options/set-rectangle":"<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-rectangle\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"set-rectangle\"\n>\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}\n      <span class=\"required-text\">{{lang 'common.required'}}</span>\n    {{/if}}\n  </div>\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"form-label rectangle\" data-product-attribute-value=\"{{id}}\">\n        <input\n          class=\"form-input form-rectangle\"\n          name=\"attribute[{{../id}}]\"\n          id=\"attribute-{{id}}\"\n          type=\"radio\"\n          value=\"{{id}}\"\n          {{#if selected}}\n            checked\n            data-default\n          {{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\"\n        >\n        <span class=\"rectangle-text form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n    {{#unless required}}\n      <label class=\"form-label rectangle\">\n        <input\n          class=\"form-input form-rectangle\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"0\"\n          {{#if selected}}checked{{/if}}\n          aria-required=\"{{required}}\"\n        >\n        <span class=\"rectangle-text form-label-text\">\n          {{lang 'core.product.none'}}\n        </span>\n      </label>\n    {{/unless}}\n  </div>\n</div>\n","core/forms/options/set-select":"<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-select\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"set-select\"\n>\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}\n        <span class=\"required-text\">{{lang 'common.required'}}</span>\n      {{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <div class=\"form-select-wrapper\">\n        <select\n          class=\"form-input form-select\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{id}}]\"\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\"\n        >\n          <option\n            value=\"\"\n            {{#any values selected=true}}\n            {{else}}\n              selected\n            {{/any}}\n          >\n            {{lang 'product.pick_one'}}\n          </option>\n          {{#each values}}\n            <option\n              value=\"{{id}}\"\n              {{#if selected}}selected{{/if}}\n              data-product-attribute-value=\"{{id}}\"\n            >\n              {{label}}\n            </option>\n          {{/each}}\n          {{#unless required}}\n            <option\n              value=\"{{id}}\"\n              {{#if selected}}selected{{/if}}\n              data-product-attribute-value=\"{{id}}\"\n            >\n              {{lang 'core.product.none'}}\n            </option>\n          {{/unless}}\n        </select>\n      </div>\n    </span>\n  </label>\n</div>\n","core/forms/options/swatch":"<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-swatch\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-swatch-selector\n  data-product-attribute=\"swatch\"\n>\n  <div class=\"form-field-title\">\n    {{display_name}}\n    <span class=\"swatch-value\" data-swatch-value>\n      {{lang 'forms.swatches_none_selected'}}\n    </span>\n    {{#if required}}\n      <span class=\"required-text\">{{lang 'common.required'}}</span>\n    {{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label\n        class=\"swatch-wrap\"\n        data-swatch-value=\"{{label}}\"\n        data-product-attribute-value=\"{{id}}\"\n      >\n        <span class=\"form-label-text\">{{label}}</span>\n\n        <input\n          class=\"form-input swatch-radio\"\n          id=\"attribute-{{id}}\"\n          type=\"radio\"\n          name=\"attribute[{{../id}}]\"\n          value=\"{{id}}\"\n          {{#if selected}}\n            checked\n            data-default\n          {{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\"\n        >\n\n        <span\n          class=\"\n            swatch\n            {{#if data.[1]}}two-colors{{/if}}\n            {{#if data.[2]}}three-colors{{/if}}\"\n          >\n          {{#if pattern}}\n            <span\n              class=\"swatch-color swatch-pattern\"\n              style=\"background-image: url('{{getImage image 'core-swatch'}}');\"\n            >\n            </span>\n          {{/if}}\n\n          {{#if data.[0]}}\n            <span\n              class=\"swatch-color\"\n              style=\"background-color: #{{data.[0]}}\"\n            >\n            </span>\n          {{/if}}\n\n          {{#if data.[1]}}\n            <span\n              class=\"swatch-color secondary\"\n              style=\"background-color: #{{data.[1]}}\"\n            >\n            </span>\n          {{/if}}\n\n          {{#if data.[2]}}\n            <span\n              class=\"swatch-color tertiary\"\n              style=\"background-color: #{{data.[2]}}\"\n            >\n            </span>\n          {{/if}}\n        </span>\n\n        {{#if pattern}}\n          <span class=\"swatch-pattern-expanded\">\n            <img\n              class=\"swatch-pattern-image\"\n              src=\"{{getImage image 'core-swatch'}}\"\n            >\n          </span>\n        {{/if}}\n\n      </label>\n    {{/each}}\n    {{#unless required}}\n      <label\n        class=\"swatch-wrap swatch-none\"\n        data-swatch-value=\"{{lang 'core.product.none'}}\"\n        data-product-attribute-value=\"{{id}}\"\n      >\n        <span class=\"form-label-text\">{{lang 'core.product.none'}}</span>\n        <input\n          class=\"form-input swatch-radio\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"0\"\n          {{#if prefill '==' 0}}checked{{/if}}\n          aria-required=\"{{required}}\"\n        >\n        <span class=\"swatch\">\n          <svg\n            width=\"50\"\n            height=\"50\"\n            viewBox=\"0 0 50 50\"\n            xmlns=\"http://www.w3.org/2000/svg\"\n          >\n            <path d=\"M46.717.5633l2.7197 2.7196L3.283 49.4366.5632 46.717\" fill-rule=\"nonzero\" fill=\"#D0021B\"/>\n          </svg>\n        </span>\n      </label>\n    {{/unless}}\n  </div>\n</div>\n","core/forms/options/textarea":"<div\n  class=\"\n    form-field\n    form-field-options\n    form-field-textarea\n    {{#if required}}\n      form-required\n    {{/if}}\n  \"\n  data-product-attribute=\"textarea\"\n>\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}\n        <span class=\"required-text\">{{lang 'common.required'}}</span>\n      {{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <textarea\n        class=\"form-input\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        rows=\"4\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\"\n      >\n        {{prefill}}\n      </textarea>\n    </span>\n  </label>\n</div>\n","components/common/modal-close":"<a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'cart.modal_close'}}\">\n  <span aria-hidden=\"true\">\n    <svg class=\"icon icon-close\"><use xlink:href=\"#icon-close\" /></svg>\n  </span>\n</a>\n","components/common/login-for-pricing":"<span class=\"login-for-pricing\">\n  <a class=\"login-for-pricing-link link\" href=\"{{urls.auth.login}}\">\n    {{lang 'common.login_for_pricing'}}\n  </a>\n</span>\n","components/wishlist/wishlist-lists":"<div class=\"override-dropdown\" data-dropdown-close>\n {{> components/common/modal-close}}\n</div>\n<div>\n  <span class=\"wishlist-title\">\n    {{lang \"product.choose_wishlist\"}}\n  </span>\n  <ul class=\"wishlist\">\n    {{#if customer.wishlists}}\n      {{#each customer.wishlists}}\n        <li class=\"wishlist-item\">\n          <a\n            href=\"{{../product.add_to_wishlist_url}}&wishlistid={{id}}\"\n            data-wishlist=\"{{view_url}}\"\n            data-name=\"{{name}}\"\n          >\n            {{name}}\n          </a>\n        </li>\n      {{/each}}\n    {{/if}}\n    <li class=\"wishlist-item\">\n      <a class=\"create-wishlist\" href=\"#create-wishlist\" data-create-wishlist>\n        <span class=\"create-wishlist-plus\"></span>\n        <span class=\"create-wishlist-text\">\n          {{lang \"product.create_wishlist\"}}\n        </span>\n      </a>\n    </li>\n  </ul>\n</div>\n"}