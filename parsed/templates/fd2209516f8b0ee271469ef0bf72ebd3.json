{"components/product-catalog/catalog-header":"<header\n  class=\"\n    category-header\n    listing-header\n    page-header\n    {{#if category.image}}has-image{{/if}}\n  \"\n>\n  {{#if catalog_type.image}}\n    {{#unless theme_settings.hide_listing_image}}\n      <div\n        class=\"\n          category-banner\n          {{#if catalog_type '===' brand}}brand-banner{{/if}}\n        \"\n      >\n        <div\n          class=\"category-banner-image\"\n          style=\"background-image: url('{{getImage catalog_type.image 'zoom'}}');\"\n        >\n        </div>\n        <div class=\"category-banner-caption\">\n          <h1 class=\"section-title\">{{catalog_type.name}}</h1>\n        </div>\n      </div>\n    {{else}}\n      <h1 class=\"section-title banner-image-hidden\">{{catalog_type.name}}</h1>\n    {{/unless}}\n  {{else}}\n    <h1 class=\"section-title\">{{catalog_type.name}}</h1>\n  {{/if}}\n\n  {{#if catalog_type.description}}\n    <div class=\"listing-description\">\n      <div class=\"container container-small\">\n        {{{catalog_type.description}}}\n      </div>\n    </div>\n  {{/if}}\n</header>\n"}